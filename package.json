  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts-wankara": "^1.0.1"
  },
  "scripts": {
    "start:trade": "cross-env NODE_PATH=. app=trade PORT=3004 react-scripts start",
